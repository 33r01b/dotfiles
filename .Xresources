Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.rgba: rgb
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.font: xft:JetBrains Mono:style=Regular:size=10,xft:JetBrainsMono Nerd Font Mono:size=10
URxvt.boldFont: xft:JetBrains Mono:style=Bold:size=10,xft:JetBrainsMono Nerd Font Mono:size=10
URxvt.italicFont: xft:JetBrains Mono:style=Italic:size=10,xft:JetBrainsMono Nerd Font Mono:size=10
URxvt.boldItalicFont: xft:JetBrains Mono:style=Bold Italic:size=10,xft:JetBrainsMono Nerd Font Mono:size=10

URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.letterSpace: 0
URxvt.lineSpace: 0

URxvt.iso14755:        false
URxvt.iso14755_52:     false

URxvt.clipboard.autocopy: true
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.urgentOnBell: true
URxvt.depth: 24

! special
*foreground: #DADAE8
*background: #1E1E28
*cursorColor: #B1E3AD

! black
*color0: #6E6C7E
*color8: #6E6C7E

! red
*color1: #E38C8F
*color9: #E38C8F

! green
*color2: #B1E3AD
*color10: #B1E3AD

! yellow
*color3: #EBDDAA
*color11: #EBDDAA

! blue
*color4: #A4B9EF
*color12: #A4B9EF

! magenta
*color5: #C6AAE8
*color13: #C6AAE8

! cyan
*color6: #E5B4E2
*color14: #E5B4E2

! white
*color7: #DADAE8
*color15: #DADAE8

